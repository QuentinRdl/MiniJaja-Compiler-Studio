class C {
  int x;
  int y = 10;
  final int VAL = 5;
  boolean b1 = true;
  boolean b2 = false;

  int  somme( int max) {
  	int t = VAL;

	while ( max > 0) {
		t += max;
		max = max - 1;
	};
	return t;
   };

   boolean test(int a, int b) {
	return a > b;
   };

   void f(int z){
   	x = y * z;
	x = x / z;
	x++;
   };

   main {
 	if (test(y,9) && (b1 || (!b2))) {
		x = somme(2);
		write("x (8) = ");
		writeln(x);
	} else {
		x++;
	};
	f(10);
	write("x (11) = ");
	writeln(x);
   }
}